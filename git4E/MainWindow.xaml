<Window x:Class="git4E.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:git4E"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="25*"/>
            <RowDefinition Height="72*"/>
            <RowDefinition Height="70*"/>
            <RowDefinition Height="267*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="29*"/>
            <ColumnDefinition Width="188*"/>
            <ColumnDefinition Width="17*"/>
            <ColumnDefinition Width="204*"/>
            <ColumnDefinition Width="362*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="stampa_lbl" Content="Label" Margin="10,10,10,10" Grid.Column="1" Grid.Row="1"/>
        <Label x:Name="nome_lbl" Grid.Column="3" Content="Label" Grid.Row="1" Margin="10,10,10,10"/>
        <Button x:Name="stampa_bt" Grid.Column="1" Content="Button" Margin="10,10,10,24" Grid.Row="2" Click="stampa_bt_Click"/>

    </Grid>
</Window>
